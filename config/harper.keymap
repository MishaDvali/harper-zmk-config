        compatible = "zmk,keymap";

        default {
            bindings = <
&kp ESC        &kp N1     &kp N2     &kp N3    &kp N4    &kp N5         &kp N6          &kp N7     &kp N8  &kp N9     &kp N0      &none
&kp GRAVE      &kp Q      &kp W      &kp E     &kp R     &kp T          &kp Y           &kp U      &kp I   &kp O      &kp P       &kp LEFT_BRACKET
&kp BACKSPACE  &kp A      &kp S      &kp D     &kp F     &kp G          &kp H           &kp J      &kp K   &kp L      &kp SEMI    &kp SQT
&kp LSHFT      &kp Z      &kp X      &kp C     &kp V     &kp B          &kp C_MUTE      &none      &kp N   &kp M      &kp COMMA   &kp DOT
&kp FSLH       &kp RSHFT  &kp LCTRL  &kp LGUI  &kp LALT  &mo 0   &mo 1  &kp LEFT_SHIFT  &kp LCTRL  &mo 2   &kp SPACE  &kp ESCAPE  &mo 0             &kp RALT
                                                                        &kp RGUI                                                                    &kp RCTRL
